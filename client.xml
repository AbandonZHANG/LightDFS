<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>  
    <comment>客户端配置文件</comment>
    <!-- 操作系统,输入： windows / linux -->
    <entry  
        key="os">windows</entry>
	<!-- 主控节点ip地址 -->	
    <entry  
        key="namenodeip">172.30.59.25</entry>
	<!-- 主控节点端口号 -->	
    <entry  
        key="namenodeport">2020</entry>
	<!-- 数据块大小(MB) -->	
    <entry  
        key="blocksize">64</entry>
</properties>
